syntax: regexp

# Extensions to ignore
\.o$
\.pyc$
\.a$
\.so$

# ignore the emacs backup files
.~$

# ignore the emacs temporary files
\#.*\#$
\#.*$

# ignore the generated stderr/stdout actual files
